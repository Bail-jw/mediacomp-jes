\newif\if@chap@noclearpage
\def\ph@@@chapter{%
  \if@chap@noclearpage
  \else
    \clearpage
  \fi
  \global\@chap@noclearpagefalse
  \suppressfloats[t]%
  \setcounter{example}{0}%
  \@afterindentfalse
  \secdef\@chapter\@schapter%
}%
\let\chapter\ph@@@chapter
\def\@chapter[#1]#2{%
  \refstepcounter{chapter}%
  \ifparttoc@loaded@@
    \edef\tmp@@{\thechapter}%
    \expandafter\write@part@toc\expandafter{\tmp@@}{#1}%
  \fi
  \addcontentsline{toc}{chapter}% Must be here!
          {\protect\numberline{\thechapter}#1}%
  \chaptermark{#1}%
  \set@chapter@title{#2}{}%
}%

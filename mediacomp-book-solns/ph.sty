}%
\def\extra@chapter@skip{\relax}
\newcommand{\set@chapter@title}[2]{%
  \noindent
  \thispagestyle{plain}%
  \ifx#2U%
    \nochapter@numbertrue
  \fi
  \ifnochapter@number
  \else
    \ifin@@appendix@
      {\phfont@CN%
         A\kern0.5em P\kern0.5em P\kern0.5em E\kern0.5em %
         N\kern0.5em D\kern0.5em I\kern0.5em X%
         \kern1.5em\thechapter
      }%
    \else
      {\phfont@CN%
         C\kern0.5em H\kern0.5em A\kern0.5em P\kern0.5em %
         T\kern0.5em E\kern0.5em R%
         \kern1.5em\thechapter
      }%
    \fi
    \par
    \vspace{1pc}%
  \fi
  \global\nochapter@numberfalse
  \noindent{\raggedright\phfont@CT#1\par}%
  \vspace{2pc}\par%
  % Set part minitoc
  \ifchaptoc@loaded@@
    \ifdonot@minitoc@here
      \global\donot@minitoc@herefalse
    \else
      \check@chap@toc@notempty
      \if@chap@toc@notempty
\begin{chaptoc@@}
  \open@chap@tocfile
\end{chaptoc@@}
      \else
        \open@chap@tocfile
        \message{ skipping \jobname.c\thechapter }%
      \fi
      \vskip 2pc plus 2pt minus 1pt\par
    \fi
  \fi
  \extra@chapter@skip
  \noindent\ignorespaces
}%

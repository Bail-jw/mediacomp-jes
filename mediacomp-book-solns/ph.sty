\newskip\temp@@skip
\newskip\last@@skip
\newif\ifuse@last@@skip\use@last@@skipfalse
\newif\ifsuppress@before@skip\suppress@before@skipfalse
\newcommand{\skip@break}[5][]{%
  \ifsuppress@before@skip
    \global\suppress@before@skipfalse
    \global\last@@skip=0pt
  \else
    \temp@@skip=#2 plus #3 minus #4\relax
    \par
    \ifuse@last@@skip
      \skip0=\last@@skip
    \else
      \skip0=\lastskip
    \fi
    \global\use@last@@skipfalse
    \ifdim\skip0<\temp@@skip
      \ifdim\skip0=0pt \else\vskip-\skip0\fi
      \penalty#5\relax
      \vskip\temp@@skip
      \if#1i%
      \else
        \global\last@@skip=\temp@@skip
      \fi
    \fi
  \fi
}
